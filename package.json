{
  "name": "bz-mathlib",
  "version": "0.0.4",
  "description": "Delightul methods for processing numbers",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel ./src --out-dir ./lib ",
    "p": "npm run build && npm publish",
    "test": "jest --coverage && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BaconZhang/MathLib.git"
  },
  "keywords": [
    "javascript",
    "number"
  ],
  "author": "BaconZhang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BaconZhang/MathLib/issues"
  },
  "homepage": "https://github.com/BaconZhang/MathLib#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "codecov": "^3.1.0",
    "husky": "^1.0.0-rc.14",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "regenerator-runtime": "^0.12.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
